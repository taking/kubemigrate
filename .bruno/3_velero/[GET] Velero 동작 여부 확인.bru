meta {
  name: [POST] Velero 동작 여부 확인
  type: http
  seq: 1
}

post {
  url: {{host_ip}}:{{host_port}}/api/v1/velero/health
  body: json
  auth: inherit
}

body:json {
  {
    "kubeconfig": {
      "kubeconfig": "{{base64_local_kubeconfig}}"
    },
    "minio": {
      "endpoint": "{{minio_url}}",
      "accessKey": "{{minio_accesskey}}",
      "secretKey": "{{minio_secretkey}}",
      "useSSL": false
    }
  }
}

settings {
  encodeUrl: true
}
