meta {
  name: 1_minio 연결 상태 조회
  type: http
  seq: 1
}

post {
  url: {{host_ip}}:{{host_port}}/api/v1/minio/health
  body: json
  auth: inherit
}

body:json {
  {
      "endpoint": "{{minio_url}}",
      "accessKey": "{{minio_accesskey}}",
      "secretKey": "{{minio_secretkey}}",
      "useSSL": false
  }
}

settings {
  encodeUrl: true
}
