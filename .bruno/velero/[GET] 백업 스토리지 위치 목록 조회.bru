meta {
  name: [GET] 백업 스토리지 위치 목록 조회
  type: http
  seq: 5
}

get {
  url: {{host_ip}}:{{host_port}}/api/v1/velero/backup-storage-locations
  body: json
  auth: inherit
}

body:json {
  {
    "minio": {
      "endpoint": "{{minio_url}}",
      "accessKey": "{{minio_accesskey}}",
      "secretKey": "{{minio_secretkey}}",
      "useSSL": false
    },
    "kubernetes": {
      "kubeconfig": "{{base64_local_kubeconfig}}"
    }
  }
}

settings {
  encodeUrl: true
}
