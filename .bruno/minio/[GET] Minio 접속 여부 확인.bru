meta {
  name: [GET] Minio 접속 여부 확인
  type: http
  seq: 1
}

get {
  url: {{host_ip}}:{{host_port}}/api/v1/minio/health
  body: json
  auth: inherit
}

body:json {
  {
      "endpoint": "{{minio_url}}",
      "accessKey": "{{minio_accesskey}}",
      "secretKey": "{{minio_secretkey}}"
  }
}

settings {
  encodeUrl: true
}
